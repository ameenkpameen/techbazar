    <script>
        function validate(addressId) {
            addressId = addressId
            Swal.fire({
                title: 'Are you sure?',
                text: "Are you want to Delete this address!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, Delete it!'
            }).then((result) => {
              if (result.isConfirmed) {
                $.ajax({
                    url: '/deleteaddress',
                    data: {
                        addressId: addressId
                    },
                    method: 'post',
                    success: (response) => {
                        
                            Swal.fire({
                                title: 'Deleted!',
                                text: 'Address has been Deleted.',
                                icon: 'success',
                                showConfirmButton: false
                            })
                            setTimeout(() => {
                                
                                $('#tab-address').load('/addresses #tab-address');
                                Swal.close();
                            }, 1000);
                       
                    }
                })
              }
            })
        }
    </script>





