        <script>
            function incrementQuantity(productId,proPrice, count) {
                let quantity = parseInt(document.getElementById(productId).innerHTML)
                count = parseInt(count)
                if(quantity <= 1 && count == -1){

                }else{
                    $.ajax({
                        url: '/incrementquantity',
                        data: {
                            productId: productId,
                            count: count,
                            proPrice:proPrice
                        },
                        method: 'post',
                        success: (response) => {
                            console.log(response);
                            document.getElementById(productId).innerHTML = parseInt(quantity) + count
                            document.getElementsByClassName(productId)[1].innerHTML = response.producttotal
                            document.getElementById("cartTotal").innerHTML = '₹'+response.totalPrice
                            document.getElementById("cartstotal").innerHTML = '₹'+response.totalPrice,
                            document.getElementsByClassName(productId)[0].innerHTML = response.stockStatus
                        }
                    })
                }
            }
        </script>

<script>
    function validate(productId) {
        productId = productId
        Swal.fire({
            title: 'Are you sure?',
            text: "Are you want to remove this from your cart!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, Remove it!'
        }).then((result) => {
          if (result.isConfirmed) {
            $.ajax({
                url: '/removefromcart',
                data: {
                    productId: productId
                },
                method: 'post',
                success: (response) => {
                    
                        Swal.fire({
                            title: 'Removed!',
                            text: 'Item has been Removed.',
                            icon: 'success',
                            showConfirmButton: false
                        })
                        setTimeout(() => {
                            location.reload()
                        }, 500);
                   
                }
            })
          }
        })
    }
</script>

            






